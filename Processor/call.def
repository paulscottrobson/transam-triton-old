CD 			17 	"CALL @2@1"		FETCHWORD();PUSH(pc >> 8);PUSH(pc & 0xFF);SETPC(temp16)
C4-FC,8 	10  "CALL @C,@2@1"	FETCHWORD();if (@T) { PUSH(pc >> 8);PUSH(pc & 0xFF);SETPC(temp16);TSTATE(7); }

C9 			10 	"RET"			temp16 = POP();temp16 = temp16 | (((WORD16)POP()) << 8);SETPC(temp16)
C0-F8,8 	5 	"RET @C"		if (@T) { temp16 = POP();temp16 = temp16 | (((WORD16)POP()) << 8); TSTATE(6); SETPC(temp16); }

C7-FF,8		11 	"RST @O"		PUSH(pc >> 8);PUSH(pc & 0xFF);SETPC(0x@O)

