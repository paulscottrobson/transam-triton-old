//*******************************************************************************************************
//*******************************************************************************************************
//
//							 				8 bit Load Group
//
//*******************************************************************************************************
//*******************************************************************************************************

40-47 		4 	"LD B,@N"		b = @R
48-4F 		4 	"LD C,@N"		c = @R
50-57 		4	"LD D,@N"		d = @R
58-5F 		4	"LD E,@N"		e = @R
60-67 		4	"LD H,@N"		h = @R
68-6F 		4	"LD L,@N"		l = @R
78-7F 		4	"LD A,@N"		a = @R

06-2E,8 	7 	"LD @N,@1"		@R = fetch()
3E 			7 	"LD A,@1"		a = fetch()

70-75 		7 	"LD (HL),@N"	writeMemory(HL,@R)
77 			7 	"LD (HL),A"		writeMemory(HL,a)

36			10 	"LD (HL),@1"	writeMemory(HL,fetch())

0A 			7 	"LD A,(BC)"		a = readMemory(BC)
1A 			7 	"LD A,(DE)"		a = readMemory(DE)
3A 			13 	"LD A,(@2@1)"	FETCHWORD();a = readMemory(temp16)

02 			7 	"LD (BC),A"		writeMemory(BC,a)
12 			7 	"LD (DE),A"		writeMemory(DE,a)
32 			13 	"LD (@2@1),A"	FETCHWORD();writeMemory(temp16,a)
